{
    "name": "shinjam-blog",
    "author": "shinjam <nevvjann@gmail.com>",
    "version": "1.0.0",
    "description": "Shinjam DevLog",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/runtime-corejs3": "^7.9.6",
        "algoliasearch": "^3.35.1",
        "autoprefixer": "^9.8.0",
        "fs": "0.0.1-security",
        "imagemin": "^7.0.1",
        "imagemin-keep-folder": "^5.3.2",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-optipng": "^7.1.0",
        "instantsearch.js": "^4.5.0",
        "js-yaml": "^3.13.1",
        "lodash.escaperegexp": "^4.1.2",
        "parcel-bundler": "1.12.4",
        "path": "^0.12.7",
        "postcss-modules": "^2.0.0",
        "prettier": "1.19.1",
        "replace-in-file": "^6.0.0",
        "sass": "^1.26.5",
        "typescript": "^3.9.3"
    },
    "scripts": {
        "run": "npm run jekyll:build %% jekyll serve",
        "start": "npm run build && jekyll serve",
        "build": "rimraf .cache && rimraf .jekyll-cache && npm run bundle && npm run jekyll:build",
        "jekyll:build": "bundle exec jekyll build",
        "bundle": "rimraf public && npm run img & npm run css & npm run scripts",
        "img": "node imagemin.js",
        "css": "parcel build _assets/scss/main.scss --public-url ./ --out-dir public --no-source-maps",
        "scripts": "npm run scripts:js & npm run scripts:ts",
        "scripts:js": "parcel build _assets/js/*.js --out-dir public --public-url ./ --no-source-maps",
        "scripts:ts": " parcel build _assets/ts/app.ts --out-dir public --public-url ./ --no-source-maps"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ShinJam/shinjam.github.io.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ShinJam/shinjam.github.io/issues"
    },
    "homepage": "https://github.com/ShinJam/shinjam.github.io#readme"
}
